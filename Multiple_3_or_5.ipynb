      "2318\n"
     ]
    }
   ],
   "source": [
    "lst = list(range(1, 100))\n",
    "\n",
    "def compute(lst):\n",
    "    total = 0\n",
    "    for i in lst:\n",
    "        if i % 3 == 0 or i % 5 == 0:\n",
    "            total += i\n",
    "    return total\n",
    "\n",
    "print(compute(lst))"
   ]
  }
